server {

  listen 9000;
  server_name _;
  client_max_body_size	20M;

  root /var/www/html/monitor/;
  index index.php;
  add_header 'Access-Control-Allow-Origin' '*';

  location ~ \.php$ {
    include /etc/nginx/fastcgi.conf;
    fastcgi_pass unix:/run/php/php{{MONITORING_PHP_VERSION}}-fpm.sock;
  }
}
